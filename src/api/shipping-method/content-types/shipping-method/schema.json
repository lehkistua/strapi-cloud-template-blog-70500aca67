{
  "kind": "collectionType",
  "collectionName": "shipping-methods",
  "info": {
    "singularName": "shipping-method",
    "pluralName": "shipping-methods",
    "displayName": "Shipping Method",
    "description": "Способы доставки"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "cost": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "estimatedDeliveryTime": {
      "type": "string"
    }
  }
}